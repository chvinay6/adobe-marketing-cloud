<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
	xmlns:nt="http://www.jcp.org/jcr/nt/1.0" xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
	jcr:primaryType="nt:unstructured" jcr:title="aemsp_mostRead" width="500"
	sling:resourceType="cq/gui/components/authoring/dialog">
	<content jcr:primaryType="nt:unstructured" class="coral-TabPanel-content"
		sling:resourceType="granite/ui/components/foundation/container">
		<items jcr:primaryType="nt:unstructured">
			<headline jcr:primaryType="nt:unstructured" name="./headline"
				fieldLabel="aemsp_mostReadHeadlineLabel" sling:resourceType="granite/ui/components/coral/foundation/form/textfield">
			</headline>

			<aemsp_mostReadMaxNumber
				jcr:primaryType="nt:unstructured"
				sling:resourceType="granite/ui/components/coral/foundation/form/numberfield"
				fieldLabel="aemsp_mostReadMaxNumberLabel" required="{Boolean}true"
				value="5" name="./maxNumber" />

		</items>
	</content>
</jcr:root>

