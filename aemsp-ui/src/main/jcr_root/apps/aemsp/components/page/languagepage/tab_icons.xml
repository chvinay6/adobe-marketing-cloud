									fieldDescription="aemsp_faviconDescription" fieldLabel="Favicon"
									name="./favIconPath" rootPath="/content/dam" cq:showOnCreate="{Boolean}true"
									enable-regex-validation="{Boolean}true" regex="^$|^.*\\.ico$"
